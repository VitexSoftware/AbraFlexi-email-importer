{
    "name": "vitexsoftware/imap2abraflexi",
    "description": "Import Invoices from imap Inbox to AbraFlexi",
    "authors": [
        {
            "name": "Vitex",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "ext-imap": "*",
        "ext-zip": "*",
        "spojenet/flexibee": "2.0.x-dev",
        "vitexsoftware/flexibee-bricks": "dev-abraflexi",
        "php-imap/php-imap": "dev-master",
        "lightools/xml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "AbraFlexi\\Imap2AF\\": "src/Imap2AF/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\AbraFlexi\\Imap2AF\\": "tests/src/Imap2AF/"
        }
    }
}
