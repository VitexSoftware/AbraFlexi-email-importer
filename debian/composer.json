{
    "name": "deb/flexibee-email-importer",
    "description": "Invoice Matcher for FlexiBee written in PHP",
    "type": "project",
    "version": "0.8",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "vendor-dir": "/var/lib/flexibee-email-importer"
    },
    "require": {
        "ext-imap": "*",
        "ext-zip": "*",
        "deb/flexibee-bricks": "*",
        "php-imap/php-imap": "dev-master",
        "lightools/xml": "*"
    },
    "autoload": {
        "psr-4": {
            "FlexiPeeHP\\Imap2FB\\": "/usr/share/flexibee-email-importer/Imap2FB/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "/usr/share/php/EaseCore"
        },
        {
            "type": "path",
            "url": "/usr/share/php/EaseHtml"
        },
        {
            "type": "path",
            "url": "/usr/share/php/FlexiPeeHP"
        },
        {
            "type": "path",
            "url": "/usr/share/php/EaseBricks"
        },
        {
            "type": "path",
            "url": "/usr/share/php/FlexiPeeHPBricks"
        }
    ]
}
